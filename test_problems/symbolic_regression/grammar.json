{
    "grammar": {
        "non_terminals": ["expr", "op", "pre-op", "var"],
        "terminals": ["sin","cos","exp","log","+","-","/","*","^","X","1.0","(",")"],
        "start":"expr",
        "production_rules": {
            "expr": ["expr;op;expr", "(;expr;op;expr;)","pre-op;(;expr;)","var"],
            "op": ["+","-","/","*"],
            "pre-op": ["sin","cos","log"],
            "var": ["X"]
        }
    }
}
